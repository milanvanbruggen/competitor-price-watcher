{
  "domain": "deplasticwinkel.nl",
  "categories": {
    "square_meter_price": {
      "steps": [
        {
          "type": "click",
          "selector": "#cookie-policy-accept-button",
          "description": "Accept cookies"
        },
        {
          "type": "wait",
          "duration": "default"
        },
        {
          "selector": "#Thickness",
          "type": "select",
          "unit": "mm",
          "value": "{thickness}"
        },
        {
          "selector": "#calcWidth",
          "type": "input",
          "unit": "cm",
          "value": "{width}"
        },
        {
          "duration": "default",
          "type": "wait"
        },
        {
          "selector": "#calcLength",
          "type": "input",
          "unit": "cm",
          "value": "{length}"
        },
        {
          "duration": "default",
          "type": "wait"
        },
        {
          "includes_vat": false,
          "selector": "#FormulaPrice_ProductInfo_SPAN",
          "type": "read_price"
        }
      ]
    }
  }
}