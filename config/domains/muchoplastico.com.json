{
    "domain": "muchoplastico.com",
    "categories": {
        "square_meter_price": {
            "steps": [
                {
                    "type": "select",
                    "selector": "#custom_select_thickness .value-thickness",
                    "option_container": "#custom_select_thickness .list-thickness ul",
                    "option_selector": "li[data-value='{value}']",
                    "value": "{thickness}",
                    "unit": "mm"
                },
                {
                    "type": "wait",
                    "duration": "long"
                },
                {
                    "type": "click",
                    "selector": "#but_custom_cut"
                },
                {
                    "type": "wait",
                    "duration": "default"
                },
                {
                    "type": "input",
                    "selector": "#input_y",
                    "value": "{length}",
                    "unit": "mm"
                },
                {
                    "type": "wait",
                    "duration": "short"
                },
                {
                    "type": "input",
                    "selector": "#input_x",
                    "value": "{width}",
                    "unit": "mm"
                },
                {
                    "type": "wait",
                    "duration": "short"
                },
                {
                    "type": "click",
                    "selector": "#but_next"
                },
                {
                    "type": "wait",
                    "duration": "default"
                },
                {
                    "type": "read_price",
                    "selector": "#pvp_config",
                    "includes_vat": true
                }
            ]
        }
    }
}