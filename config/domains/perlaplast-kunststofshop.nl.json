{
    "domain": "perlaplast-kunststofshop.nl",
    "steps": [
        {
            "selector": ".itoris-dynamicoptions-template .is-detached:nth-child(1) .product-custom-option",
            "type": "input",
            "unit": "mm",
            "value": "{width}"
        },
        {
            "duration": 1000,
            "type": "wait"
        },
        {
            "selector": ".itoris-dynamicoptions-template .is-detached:nth-child(2) .product-custom-option",
            "type": "input",
            "unit": "mm",
            "value": "{length}"
        },
        {
            "duration": 1000,
            "type": "wait"
        },
        {
            "includes_vat": false,
            "selector": ".price-excluding-tax .price",
            "type": "read_price"
        }
    ]
}