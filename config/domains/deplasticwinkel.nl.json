{
    "domain": "deplasticwinkel.nl",
    "categories": {
        "square_meter_price": {
            "steps": [
                {
                    "type": "select",
                    "selector": "#Thickness",
                    "value": "{thickness}",
                    "unit": "mm"
                },
                {
                    "type": "input",
                    "selector": "#calcWidth",
                    "value": "{width}",
                    "unit": "cm"
                },
                {
                    "type": "wait",
                    "duration": "default"
                },
                {
                    "type": "input",
                    "selector": "#calcLength",
                    "value": "{length}",
                    "unit": "cm"
                },
                {
                    "type": "wait",
                    "duration": "default"
                },
                {
                    "type": "read_price",
                    "selector": "#FormulaPrice_ProductInfo_SPAN",
                    "includes_vat": false
                }
            ]
        }
    }
} 