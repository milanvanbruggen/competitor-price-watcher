{
  "categories": {
    "shipping": {
      "steps": [
        {
          "selector": "#CybotCookiebotDialogBodyButtonDecline",
          "type": "click"
        },
        {
          "duration": "short",
          "type": "wait"
        },
        {
          "selector": "#calculatorThickness",
          "type": "click"
        },
        {
          "duration": "default",
          "type": "wait"
        },
        {
          "selector": "#partControlRadioThickness .checkBoxRadioItem input[type='radio']",
          "type": "select",
          "unit": "mm",
          "value": "{thickness}"
        },
        {
          "duration": "default",
          "type": "wait"
        },
        {
          "selector": "#calculatorFigureAndSizes",
          "type": "click"
        },
        {
          "duration": "default",
          "type": "wait"
        },
        {
          "selector": "#calculatorFigure .checkBoxRadioItem:first-child label",
          "type": "click"
        },
        {
          "duration": "default",
          "type": "wait"
        },
        {
          "selector": "#sizeA",
          "type": "input",
          "unit": "mm",
          "value": "{width}"
        },
        {
          "duration": "default",
          "type": "wait"
        },
        {
          "selector": "#sizeB",
          "type": "input",
          "unit": "mm",
          "value": "{length}"
        },
        {
          "type": "blur"
        },
        {
          "duration": "default",
          "type": "wait"
        },
        {
          "selector": ".trackingIdInwinkelwagen",
          "type": "click"
        },
        {
          "duration": "default",
          "type": "wait"
        },
        {
          "selector": ".trackingIdNaarwinkelwagen",
          "type": "click"
        },
        {
          "duration": "long",
          "type": "wait"
        },
        {
          "selector": ".shoppingCartQuantity-input",
          "type": "input",
          "unit": "pieces",
          "value": "{quantity}"
        },
        {
          "duration": "long",
          "type": "wait"
        },
        {
          "default_value": 0,
          "includes_vat": false,
          "selector": "xpath=//div[@id='shoppingCartSummary']//div[contains(@class, 'table-row')]//span[contains(@class, 'table-row-label') and text()='Verzendkosten']/following-sibling::span[contains(@class, 'table-row-value')]",
          "type": "read_price"
        }
      ]
    },
    "square_meter_price": {
      "steps": [
        {
          "selector": "#CybotCookiebotDialogBodyButtonDecline",
          "type": "click"
        },
        {
          "duration": "short",
          "type": "wait"
        },
        {
          "includes_vat": false,
          "selector": "#calculatorPrice .price-default",
          "type": "read_price"
        }
      ]
    }
  },
  "domain": "voskunststoffen.nl"
}