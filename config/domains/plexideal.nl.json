{
    "domain": "plexideal.nl",
    "steps": [
        {
            "type": "input",
            "selector": "#input_length",
            "value": "{length}",
            "unit": "cm"
        },
        {
            "type": "wait",
            "duration": 1000
        },
        {
            "type": "input",
            "selector": "#input_width",
            "value": "{width}",
            "unit": "cm"
        },
        {
            "type": "wait",
            "duration": 1000
        },
        {
            "type": "read_price",
            "selector": ".price-box__price--untaxed .amount",
            "includes_vat": false
        }
    ]
} 